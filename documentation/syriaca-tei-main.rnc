  ## (personal group) describes a group of individuals treated as a single person for analytic purposes. [15.2.2. The Participant Description]
  element personGrp {
    (tei_model.pLike+ | (tei_model.personPart | tei_model.global)*),
    tei_att.global.attributes,
    tei_att.sortable.attributes,
    
    ## specifies the role of this group of participants in the interaction.
    attribute role {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    
    ## specifies the sex of the participant group.
    attribute sex {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    
    ## specifies the age group of the participants.
    attribute age {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    
    ## describes informally the size or approximate size of the group for example by means of a number and an indication of accuracy e.g. approx 200.
    attribute size {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    empty
  }
tei_place =
  
  ## contains data about a geographic location [13.3.4. Places]
  element place {
    (tei_model.headLike*,
     (tei_model.pLike*
      | (tei_model.labelLike
         | tei_model.placeStateLike
         | tei_model.eventLike)*),
     (tei_model.noteLike | tei_model.biblLike | tei_idno | tei_link)*,
     (tei_model.placeLike | tei_listPlace)*),
    tei_att.global.attributes,
    tei_att.typed.attributes,
    tei_att.editLike.attributes,
    tei_att.sortable.attributes,
    empty
  }
tei_relation =
  
  ## (relationship) describes any kind of relationship or linkage amongst a specified group of places, events, persons, objects or other items. [13.3.2.3. Personal Relationships]
  element relation {
    (tei_desc?)
    >> sch:pattern [
         id =
           "syriacaMainODD-relation-relation-resources-constraint-rule-11"
         "\x{a}" ~
         "            "
         sch:rule [
           "\x{a}" ~
           "                                "
           sch:report [
             test = "tei:*[not(@mutual) and not(@active and @passive)]"
             "Error: The relation element must contain either \x{a}" ~
             "                                @mutual or both @active and @passive together."
           ]
           "\x{a}" ~
           "                            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "syriacaMainODD-relation-reforkeyorname-constraint-assert-11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relation"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@ref or @key or @name"
             "One of the attributes  'name', 'ref' or 'key' must be supplied"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "syriacaMainODD-relation-activemutual-constraint-report-20"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relation"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@active and @mutual"
             "Only one of the attributes\x{a}" ~
             "@active and @mutual may be supplied"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "syriacaMainODD-relation-activepassive-constraint-report-21"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relation"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@passive and not(@active)"
             "the attribute 'passive'\x{a}" ~
             "	may be supplied only if the attribute 'active' is\x{a}" ~
             "	supplied"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.attributes,
    tei_att.datable.attributes,
    tei_att.editLike.attributes,
    tei_att.canonical.attribute.key,
    tei_att.sortable.attributes,
    tei_att.typed.attribute.subtype,
    
    ## (reference) An RDF property applicable to the relation. This may be a custom Syriaca.org property, such as one found at http://syriaca.org/documentation/relations.html or one from an external vocabulary such as SNAP or LAWD.
    ## Suggested values include: 1] dct:source(is derived from) ; 2] lawd:embodies(embodies) ; 3] syriaca:commemorated(commemorates) ; 4] dct:isPartOf(is part of) ; 5] syriaca:sometimesCirculatesWith(sometimes circulates with) ; 6] skos:broadMatch(has broader match with) ; 7] syriaca:hasRelationToPlace(has relation to place) ; 8] syriaca:bornAt(was born at) ; 9] syriaca:hasLiteraryConnectionToPlace(has literary connection to place) ; 10] syriaca:differentFrom(should be distinguished from) ; 11] syriaca:possiblyIdentical(is possibly identical to) ; 12] syriaca:diedAt(died at) ; 13] syriaca:sharesAttributionWith(shares attribution with) 
    attribute ref {
      list {
        (
         ## (is derived from) A related resource from which the described resource is derived (http://purl.org/dc/terms/source).
         "dct:source"
         | 
           ## (embodies) The relationship between a Written Work and the abstract Conceptual Work it embodies. (http://lawd.info/ontology/embodies).
           "lawd:embodies"
         | 
           ## (commemorates) The relationship between a Work and the Person it commemorates.
           "syriaca:commemorated"
         | 
           ## (is part of) A related resource in which the described resource is physically or logically included. (http://purl.org/dc/terms/isPartOf).
           "dct:isPartOf"
         | 
           ## (sometimes circulates with) A related resource with which the described resource may sometimes circulate, depending on the tradition.
           "syriaca:sometimesCirculatesWith"
         | 
           ## (has broader match with) A resource with a broader or more generic definition that encompasses the current resource along with others. Syriaca.org describes the broader resource using a "composite URI". SKOS definition: skos:broadMatch is used to state a hierarchical mapping link between two conceptual resources in different concept schemes. (https://www.w3.org/2009/08/skos-reference/skos.rdf#broadMatch).
           "skos:broadMatch"
         | 
           ## (has relation to place) A place to which an entity (e.g., person) has an unspecified relation.
           "syriaca:hasRelationToPlace"
         | 
           ## (was born at) The place where a person was born.
           "syriaca:bornAt"
         | 
           ## (has literary connection to place) A place to which an entity (e.g., person) has a literary connection.
           "syriaca:hasLiteraryConnectionToPlace"
         | 
           ## (should be distinguished from) Entities (in @mutual) that are not identical but have been or could be confused.
           "syriaca:differentFrom"
         | 
           ## (is possibly identical to) Entities (in @mutual) that are possibly identical to each other.
           "syriaca:possiblyIdentical"
         | 
           ## (died at) The place where a person died.
           "syriaca:diedAt"
         | 
           ## (shares attribution with) Persons (in @mutual) to whom the same works are attributed (e.g., Ephrem, Isaac, and Pseudo-Ephrem).
           "syriaca:sharesAttributionWith"
         | xsd:anyURI)+
      }
    },
    
    ## Describes the general category of relationship, which may be used in addition to ref to determine how the relation should be displayed in the interface.
    ## Suggested values include: 1] part; 2] disambiguation; 3] general; 4] mssWitnesses; 5] translationSource
    attribute type {
      
      ## A relation between a work and the larger work of which it is a part. See label.
      "part"
      | 
        ## A relation that disambiguates an entity from others.
        "disambiguation"
      | 
        ## A relation that does not belong in a more specific category.
        "general"
      | 
        ## A relation pointing to the manuscript witnesses of an edition or translation.
        "mssWitnesses"
      | 
        ## A relation pointing to the original (or other-) language source of a translation.
        "translationSource"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    (
     ## identifies the active participants in a non-mutual relationship, or all the participants in a mutual one.
     attribute active {
       list { xsd:anyURI+ }
     }?
     | 
       ## supplies a list of participants amongst all of whom the relationship holds equally.
       attribute mutual {
         list { xsd:anyURI+ }
       }?),
    
    ## supplies a name for the kind of relationship of which this is an instance.
    attribute name {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    
    ## identifies the passive participants in a non-mutual relationship.
    attribute passive {
      list { xsd:anyURI+ }
    }?,
    empty
  }
